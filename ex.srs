type X: integer[]

variable b: X
variable c: integer

function g(a: integer) -> integer
  return a
end function

// this is a comment
function f(a: integer) -> integer
  b <- [1, 42]
  c <- 42
  // also this
  while a > 0 do
    if a >= b[1] then
      a <- a - b[1]
    else
      c <- g(a)
      a <- b[1]
      b[1] <- c
    end if
  end while
  return a + b[1] + c * 2
end function


function main()
  output(f(10))
end function
