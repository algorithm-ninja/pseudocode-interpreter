function isprime(x: integer) -> integer
    if x < 2 then
        return 0
    end if

    variable i: integer <- 2

    while i*i <= x do
        if (x mod i) == 0 then
            return 0
        end if
        i <- i + 1
    end while

    return 1
end function

function main()
    for n in [0...100] do
        if isprime(n) == 1 then
            output(n)
        end if
    end for
end function
